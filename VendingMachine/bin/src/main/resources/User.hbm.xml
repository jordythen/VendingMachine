<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="com.revature.beans.User" table="user">
	<!-- We don't have to use triggers anymore with Hibernate -->
		<id name="id" column="id">
			<generator class="increment"/>
		</id>
		<!-- NAME IS JAVA AND COLUMN IS SQL -->
		<property name="firstName" column="first_name"></property>
		
		<property name="lastName" column="last_name"></property>
		
		<property name="email"></property>
		
		<property name="username"></property>
		
		<property name="password" column="passwd"></property>
		
		<property name="balance"></property>
		
		<property name="vendingMachine" column="vendingmachineID"></property>
		
	</class>
</hibernate-mapping>